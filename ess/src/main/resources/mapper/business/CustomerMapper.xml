<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.yang.ess.business.mapper.CustomerMapper">

    <resultMap id="customer" type="com.yang.ess.business.entity.Customer">
        <id column="id" jdbcType="INTEGER" property="id"/>
        <result column="name" jdbcType="VARCHAR" property="name"/>
        <result column="phone" jdbcType="VARCHAR" property="phone"/>
        <result column="address" jdbcType="VARCHAR" property="address"/>
        <result column="expense_money" jdbcType="INTEGER" property="expenseMoney"/>
        <result column="profits_money" jdbcType="INTEGER" property="profitsMoney"/>
        <result column="status" jdbcType="INTEGER" property="status"/>
        <result column="create_time" jdbcType="TIME" property="createTime"/>
    </resultMap>
</mapper>
